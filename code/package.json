{
  "name": "websockets-game-server",
  "version": "1.0.1",
  "description": "Websockets-game-server",
  "keywords": [],
  "author": "Dean Vaessen <hello@deanvaessen.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:username"
  },
  "config": {
    "port": 3000
  },
  "scripts": {
    "start": "npm run build && node postbuildglue.js && cross-env NODE_ENV=prod npm run init-back",
    "start-dev": "npm run build && node postbuildglue.js && cross-env NODE_ENV=dev npm run init-back-dev",
    "start-prod": "cross-env NODE_ENV=prod npm run init-back",
    "start-staging": "cross-env NODE_ENV=staging npm run init-back",
    "start-dev-frontend": "concurrently --kill-others \"npm run init-front-dev\" \"cross-env NODE_ENV=dev npm run init-back-dev\"",
    "init-back": "pm2 start backend/init.js --no-daemon",
    "init-back-dev": "pm2 start backend/initdev.json --no-daemon --watch",
    "init-front-dev": "webpack-dev-server --config frontend/webpack/development.js --colors --history-api-fallback --inline --progress",
    "build": "webpack --config frontend/webpack/production.js -p --progress && node postbuildglue.js",
    "lint:js": "./node_modules/.bin/eslint src; exit 0;"
  },
  "dependencies": {
  	"autoprefixer": "^6.3.7",
  	"babel-cli": "^6.18.0",
  	"babel-core": "^6.23.1",
  	"babel-eslint": "^6.1.2",
  	"babel-loader": "^6.4.0",
  	"babel-plugin-transform-runtime": "^6.9.0",
  	"babel-preset-es2015": "^6.9.0",
  	"babel-preset-es2015-webpack2": "^1.0.0",
  	"babel-preset-react": "^6.23.0",
  	"babel-preset-stage-2": "^6.11.0",
  	"babel-register": "^6.24.0",
  	"bluebird": "^3.5.0",
  	"body-parser": "^1.17.1",
  	"browser-sync": "^2.13.0",
  	"browser-sync-webpack-plugin": "^1.1.0",
  	"concurrently": "^3.4.0",
  	"cross-env": "^3.2.4",
  	"css-loader": "^0.23.1",
  	"eslint": "^3.0.1",
  	"eslint-friendly-formatter": "^2.0.5",
  	"eslint-loader": "^1.4.1",
  	"eslint-plugin-html": "^1.5.1",
  	"eslint-plugin-import": "^1.10.2",
  	"exports-loader": "^0.6.3",
  	"express": "^4.15.2",
  	"extract-text-webpack-plugin": "^2.1.0",
  	"file-loader": "^0.9.0",
  	"fs": "0.0.1-security",
  	"html-loader": "^0.4.4",
  	"html-webpack-plugin": "^2.28.0",
  	"http": "0.0.0",
  	"imports-loader": "^0.6.5",
  	"ncp": "^2.0.0",
  	"node-sass": "^3.13.1",
  	"nodemon": "^1.11.0",
  	"pm2": "^2.4.2",
  	"postcss-loader": "^0.9.1",
  	"prerender-spa-plugin": "^1.1.0",
  	"raw-loader": "^0.5.1",
  	"react": "^15.4.2",
  	"react-dom": "^15.4.2",
  	"regenerator-runtime": "^0.10.3",
  	"require-dir": "^0.3.0",
  	"sass-loader": "^4.1.1",
  	"socket.io": "^1.7.3",
  	"source-map-loader": "^0.1.5",
  	"style-loader": "^0.13.2",
  	"to-markdown": "^3.0.3",
  	"unminified-webpack-plugin": "^1.2.0",
  	"url-loader": "^0.5.7",
  	"webpack": "^2.2.1",
  	"webpack-cleanup-plugin": "^0.5.1",
  	"webpack-dev-server": "^2.4.1",
  	"webpack-merge": "^4.0.0"
  },
  "devDependencies": {
  }
}
